{
	"name": "hear-hear",
	"version": "0.0.1",
	"description": "An example receiver bot written using @discordjs/voice",
	"main": "dist/bot.js",
	"scripts": {
		"start": "node dist/bot",
		"lint": "prettier --cache --check . && eslint src --ext mjs,js,ts --cache",
		"format": "prettier --cache --write . && eslint src --ext mjs,js,ts --fix --cache",
		"build": "rimraf dist && tsc",
		"build:check": "tsc --noEmit --incremental false"
	},
	"author": "Xipu Li <xli200@usfca.edu>",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.312.0",
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"@web-std/file": "^3.0.2",
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"fluent-ffmpeg": "^2.1.2",
		"libsodium-wrappers": "^0.7.10",
		"node-crc": "^1.3.2",
		"p-limit": "^3.1.0",
		"p-throttle": "^4.1.1",
		"prism-media": "^2.0.0-alpha.0"
	},
	"devDependencies": {
		"@types/fluent-ffmpeg": "^2.1.21",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"eslint": "^8.38.0",
		"eslint-config-marine": "^9.4.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"prettier": "^2.8.7",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
